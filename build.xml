<?xml version="1.0" encoding="UTF-8"?>

<!-- Convention: Aggregating targets no prefix.  Internal targets "-" prefix.  Worker targets "_" prefix. -->
<project name="donkey" basedir=".">
    <property name="ant.dir" value="build/ant"/>
    <import file="${ant.dir}/build-project.xml"/>
    <target name="clean" depends="-clean"/>
    <target name="checkstyle" depends="-checkstyle"/>
    <target name="donkey" depends="-donkey"/>
    <target name="donkey_dist" depends="donkey, distribution"/>
    <target name="distribution" depends="-distribution"/>
</project>
